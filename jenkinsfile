pipeline{
    agent any
    tools{
        echo 'MyGO'
        echo '$(GIT_COMMIT)'
    }
    environment{
        
    }
    stages{
        stage('Compile'){
            steps{
                sh 'go build'
                echo 'Compiling the project'
                echo '$(GIT_COMMIT)'
            }
        }
        stage('test'){
            steps{
                echo 'testing the project'
            }
        }
    } 
}
