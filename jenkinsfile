pipeline{
    agent any
    
    stages{
        stage('Compile'){
            steps{
                
                echo 'Compiling the project'
                bat "git log --oneline -1 > %%f | set var=%%f"
                bat "set list = var.split('/')"
                bat "@echo off | (for %%a in (%list%) do ( echo %%a )) | @echo on"
                
                
            }
        }
        stage('test'){
            steps{
                echo 'testing the project'
            }
        }
   } 

}
